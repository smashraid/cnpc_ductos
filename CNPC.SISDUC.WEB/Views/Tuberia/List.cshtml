<!-- Modal -->
<div id="NuevoMotivo" class="modal fade" role="dialog">
    <div class="modal-dialog">
        <div class="modal-content">
            <div class="modal-header">
                <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                    <span aria-hidden="true">×</span>
                </button>
                <h4 class="modal-title" id="myModalLabel">
                    Motivo del Cambio
                </h4>
            </div>
            <div class="modal-body">
                <form name="formNuevoMotivo" role="form">
                    <div class="form-group">
                        <h6>
                            <label for="txtDescripcion" class="control-label col-sm-2">Id Tubería:</label>
                        </h6>
                        <div class="col-sm-10">
                            <input type="text" id="txtCodigoDelTubo" name="txtFecha" class="form-control" ng-model="deleteTuberia.TuberiaId" required="required">
                        </div>
                        <h6>
                            <label for="txtDescripcion" class="control-label col-sm-2">Código de Tuberia:</label>
                        </h6>
                        <div class="col-sm-10">
                            <input type="text" id="txtCodigoDelTubo" name="txtFecha" class="form-control" ng-model="deleteTuberia.CodigoDelTubo01" required="required">
                        </div>
                    </div>
                    <div class="form-group">
                        <h6>
                            <label for="txtDescripcion" class="control-label col-sm-2">Fecha:</label>
                        </h6>
                        <div class="col-sm-4">
                            <input type="date" id="txtFecha" name="txtFecha" class="form-control" ng-model="deleteTuberia.FechaOrdenServicio" required="required">
                        </div>
                    </div>
                    <div class="form-group">
                        <h6>
                            <label for="txtDescripcion" class="control-label col-sm-2">Orden de Servicio:</label>
                        </h6>
                        <div class="col-sm-4">
                            <input type="text" id="txtOrdenDeServicio" name="txtOrdenDeServicio" class="form-control" ng-model="deleteTuberia.OrdenServicio" required="required">
                        </div>
                    </div>
                    <div class="form-group">
                        <h6><label for="txtOleoducto" class="col-sm-2">Motivo:</label></h6>
                        <div class="col-sm-10">
                            <input type="text" id="txtMotivo" name="txtMotivo" class="form-control" ng-model="deleteTuberia.Motivo" required="required">
                        </div>
                    </div>

                    <p>Ingrese los datos obligatorios</p>
                </form>
            </div>
            <div class="modal-footer modal-footerTuberia">
                <button type="button" class="btn btn-primary" data-dismiss="modal" aria-label="Close" ng-click="guardarMotivo(deleteTuberia)">Aceptar</button>
            </div>
        </div>
    </div>
</div>

