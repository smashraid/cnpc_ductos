@model PagedList.IPagedList<CNPC.SISDUC.WEB.Models.RegistroInspeccionVisualModel>
@using PagedList.Mvc;
@{
    ViewBag.Title = "Edit";
}

<div id="content">
    <div class="form-group">
        <label class="col-lg-5">CODIGO: @ViewBag.oleoducto.Codigo</label>
        <label class="col-lg-4">UBICACIÓN: @ViewBag.oleoducto.Ubicacion</label>
        <label class="col-lg-3">LONGITUD TOTAL: @ViewBag.oleoducto.LongitudTotal</label>
    </div>
    <div class="form-group">
        <label class="col-lg-5">DESCRIPCION: @ViewBag.oleoducto.Descripcion</label>
        <label class="col-lg-4">LAMINA: @ViewBag.oleoducto.NoLamina</label>
        <label class="col-lg-3">PRESION: @ViewBag.oleoducto.Presion</label>
    </div>
    <div>
        <label class="tituloh1"></label>
    </div>
    <table border="0" cellpadding="0" cellspacing="0">
        <tr>
            <td>
                <div class="input-group">
                    <input type="text" class="form-control" ng-model="parameterlist.name" placeholder="Buscar" ng-keyup="firstPage()">
                    <span class="input-group-btn">
                        <button class="btn btn-danger" type="button">Buscar</button>
                    </span>
                </div>
            </td>
        </tr>
    </table>
</div>
<table class="table table-striped table-bordered" >
